{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "newLine": "lf",
    "outDir": "../distribution",
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": true
  }
}
